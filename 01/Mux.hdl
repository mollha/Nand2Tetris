/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
  Nand(a=sel, b=b, out=bsel);
  Not(in=sel, out=nsel);
  Nand(a=a, b=nsel, out=ansel);
  Nand(a=bsel, b=ansel, out=out);
}

